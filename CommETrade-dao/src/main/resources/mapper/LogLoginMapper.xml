<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhzx.dao.mapper.commom.LogLoginMapper" >
	<!-- Result Map-->
	<resultMap id="BaseResultMap" type="com.zhzx.dao.bean.commom.LogLogin" >
				<result column="id" property="id"/>
				<result column="cust_no" property="custNo"/>
				<result column="user_id" property="userId"/>
				<result column="client" property="client"/>
				<result column="ip" property="ip"/>
				<result column="mobile" property="mobile"/>
				<result column="flag" property="flag"/>
				<result column="createtime" property="createtime"/>
			</resultMap>

	<!-- log_login table all fields -->
	<sql id="Base_Column_List" >
		id,cust_no,user_id,client,ip,mobile,flag,createtime
	</sql>


	<!-- 查询条件 -->
	<sql id="Example_Where_Clause">
		where 1=1
		<trim  suffixOverrides="," >
												<if test="id != null and id != ''" >
				and id =  #{id}
			</if>
												<if test="custNo != null and custNo != ''" >
				and cust_no =  #{custNo}
			</if>
												<if test="userId != null and userId != ''" >
				and user_id =  #{userId}
			</if>
												<if test="client != null and client != ''" >
				and client =  #{client}
			</if>
												<if test="ip != null and ip != ''" >
				and ip =  #{ip}
			</if>
												<if test="mobile != null and mobile != ''" >
				and mobile =  #{mobile}
			</if>
												<if test="flag != null and flag != ''" >
				and flag =  #{flag}
			</if>
												<if test="createtime != null and createtime != ''" >
				and createtime =  #{createtime}
			</if>
					</trim>
	</sql>


	<!-- 插入记录 -->
	<insert id="insert" parameterType="com.zhzx.dao.bean.commom.LogLogin" >
		insert into log_login(id,cust_no,user_id,client,ip,mobile,flag,createtime)
 values(#{id,jdbcType=VARCHAR},#{custNo,jdbcType=VARCHAR},#{userId,jdbcType=VARCHAR},#{client,jdbcType=VARCHAR},#{ip,jdbcType=VARCHAR},#{mobile,jdbcType=VARCHAR},#{flag,jdbcType=VARCHAR},#{createtime,jdbcType=VARCHAR})
	</insert>

	<!-- 根据id，修改记录-->
	<update id="update" parameterType="com.zhzx.dao.bean.commom.LogLogin" >
		update log_login set cust_no=#{custNo,jdbcType=VARCHAR},user_id=#{userId,jdbcType=VARCHAR},client=#{client,jdbcType=VARCHAR},ip=#{ip,jdbcType=VARCHAR},mobile=#{mobile,jdbcType=VARCHAR},flag=#{flag,jdbcType=VARCHAR},createtime=#{createtime,jdbcType=VARCHAR} where id=#{id}
	</update>

	<!--  -->
	<update id="updateBySelective" parameterType="com.zhzx.dao.bean.commom.LogLogin" >
		update log_login set 
	<trim  suffixOverrides="," >
	<if test="custNo != null  and custNo != '' ">
		cust_no=#{custNo},
	</if>
	<if test="userId != null  and userId != '' ">
		user_id=#{userId},
	</if>
	<if test="client != null  and client != '' ">
		client=#{client},
	</if>
	<if test="ip != null  and ip != '' ">
		ip=#{ip},
	</if>
	<if test="mobile != null  and mobile != '' ">
		mobile=#{mobile},
	</if>
	<if test="flag != null  and flag != '' ">
		flag=#{flag},
	</if>
	<if test="createtime != null  and createtime != '' ">
		createtime=#{createtime},
	</if>
	</trim> where id=#{id}
	</update>

	<!-- 逻辑删除，将deleted 改成 1 -->
	<update id="delete" parameterType="com.zhzx.dao.bean.commom.LogLogin">
		delete 	 from log_login where id = #{id}
	</update>

	<!-- 登陆日志列表 -->
	<select id="selectById"  resultMap="BaseResultMap" parameterType="java.lang.String">
		select <include refid="Base_Column_List" /> 
	 from log_login where id = #{id}
	</select>

	<!-- 登陆日志 列表总数-->
	<select id="selectByModelCount" resultType="java.lang.Integer"  parameterType="com.zhzx.dao.model.commom.LogLoginModel">
		select count(1) from log_login
		<include refid="Example_Where_Clause"/>
		<if test="navigate.otherCondition != null and navigate.otherCondition != ''" >
			${navigate.otherCondition}
		</if>
	</select>

	<!-- 根据id查询登陆日志 -->
	<select id="selectByModel" resultMap="BaseResultMap"  parameterType="com.zhzx.dao.model.commom.LogLoginModel">
		select
		<include refid="Base_Column_List"/>
		from log_login
		<include refid="Example_Where_Clause"/>
		<if test="navigate.orderCondition != null and navigate.orderCondition != ''" >
			${navigate.orderCondition}
		</if>
	</select>


	<!-- 登陆日志 列表总数-->
	<select id="selectByMapCount" resultType="java.lang.Integer"  parameterType="java.util.Map">
		select count(1) from log_login
		<include refid="Example_Where_Clause"/>
	</select>

	<!-- 根据id查询登陆日志 -->
	<select id="selectByMap" resultMap="BaseResultMap"  parameterType="java.util.Map">
		select
		<include refid="Base_Column_List"/>
		from log_login
		<include refid="Example_Where_Clause"/>
		<if test="queryCondition != null and queryCondition != ''" >
			${queryCondition}
		</if>
		<if test="orderCondition != null and queryCondition != ''" >
			order by ${orderCondition}
		</if>
	</select>

	<!-- 分页查询登陆日志-->
	<select id="selectByModelAsPage" resultMap="BaseResultMap"  parameterType="com.zhzx.dao.model.commom.LogLoginModel">
		select
		<include refid="Base_Column_List"/>
		from (select rownum r,<include refid="Base_Column_List"/> from
				(select * from log_login
					<include refid="Example_Where_Clause"/>
					<if test="navigate.otherCondition != null and navigate.otherCondition != ''" >
					  ${navigate.otherCondition}
					</if>
					<if test="navigate.orderCondition != null and navigate.orderCondition != ''" >
					  ${navigate.orderCondition}
					</if>
				)
		)  where 1=1
		${navigate.mysqlQueryCondition}
	</select>


</mapper>   
